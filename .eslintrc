{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-param-reassign": [2, {"props": false}],
    "prefer-arrow-callback": 0,
    "react/jsx-filename-extension": 0
  },
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": ["node_modules", "modules"]
    }
  }
}
